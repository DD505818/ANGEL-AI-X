  "include": ["next-env.d.ts", "src/**/*"],
  "exclude": ["node_modules"]
    "target": "ES2020",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "types": ["jest", "@testing-library/jest-dom"]
  },
  "include": ["web/src/**/*", "page.tsx"]

